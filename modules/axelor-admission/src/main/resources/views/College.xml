<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid name="college-grid" title="College" model="com.axelor.admission.db.College">
    <field name="name" />
  </grid>

  <form name="college-form" title="College" model="com.axelor.admission.db.College">
    <panel name="mainPanel">
      <field name="name" />
    </panel>

    <panel-related field="facultyList" form-view="faculty-entry-form" />
  </form>

</object-views>
