<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<grid name="admission-process-grid" title="Admission Process"
		model="com.axelor.admission.db.AdmissionProcess">
		<field name="name" />
		<field name="fromDate" />
		<field name="toDate" />
	</grid>

	<form name="admission-process-form" title="Admission Process"
		model="com.axelor.admission.db.AdmissionProcess">
		<panel name="mainPanel">
			<field name="name" colSpan="12" />
			<field name="fromDate" />
			<field name="toDate" />
		</panel>
		<panel>
			<button name="compute" title="Compute" onClick="action-validation-check-date,action-method-admission-process-compute,save" />
		</panel>
	</form>
	
	<action-method name="action-method-admission-process-compute">
	<call class="com.axelor.admission.web.AdmissionProcessController" method="admissionProcess"/>
	</action-method>

	<action-validate name="action-validation-check-date">
		<error message="Invalid Date Select Proper Date" if="fromDate &gt; toDate"/>
	</action-validate>
	

</object-views>
