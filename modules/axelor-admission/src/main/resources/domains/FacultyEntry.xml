<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="admission" package="com.axelor.admission.db" />

  <entity name="FacultyEntry">
    <many-to-one name="faculty"  ref="com.axelor.admission.db.Faculty" required="true" title="Faculty"/>
    <integer name="seats" title="Seats"/>
    <many-to-one name="college" title="College" ref="com.axelor.admission.db.College"/>
  </entity>

</domain-models>
